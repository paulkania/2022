base = 4
count = 12
bi = 0
bv = str('')
total =''

#gives number of buckets
while base**bi < count:
	bi += 1
	print(bi)

print('\n split while','\n')
while bi >= 0:
	if count // base**bi == 0:
		bv = str(bv)+str(0)
		bi -=1
		print('continued',bv)
		continue
	if count // base**bi != 0:
		bv = str(bv) + str(count // base**bi)
		count = count - base**bi
		bi -= 1
		print(bv,count,base,bi)


print(bv)
