base = 16
count =65
bi = 0
bv = str('')
total =''

#gives number of buckets
while base**bi < count:
	bi += 1
	print(bi)

#first logic of part two should not happen in the while loop, since it's only a one-time count < base**bi edge case.
if count // base**bi == 0:
	bv = str(bv)+str(0)
	bi -=1
	print('1st edge case',bv)

while bi >= 0:
	if count // base**bi > 0:
		bv = str(bv) + str(count // base**bi)
		count = count - ((base**bi)*(count//base**bi)) #*the golden piece...the count must be changed not just by a subtraction, but by the subtraction multipled by the floor division. english g00d at 1am
		bi -= 1
		print('c1',bv,count,bi,str(count // base**bi))
		continue
	
	if count // base**bi == 0:
		bv = str(bv) + str(0)
		bi -= 1
		print('case2',bv,count,bi)
		continue #not necessary but good for uniformity.


ans = int(bv)
print(bv)
print('ans',ans)
